# Geocoding API (optional - for address search)
GEOCODE_API_KEY=your_geocode_maps_co_api_key

# Redis/Upstash (required for analytics and online presence)
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# Analytics configuration (optional - defaults provided)
# Storage key for localStorage
NEXT_PUBLIC_ANALYTICS_STORAGE_KEY=salat10_analytics

# Number of events before auto-flush (default: 10)
NEXT_PUBLIC_ANALYTICS_BATCH_SIZE=10

# Session ID key for sessionStorage
NEXT_PUBLIC_SESSION_ID_KEY=salat10_session_id

# Periodic flush interval in milliseconds (default: 3600000 = 1 hour)
NEXT_PUBLIC_ANALYTICS_FLUSH_INTERVAL=3600000
